#reader(lib "htdp-beginner-reader.ss" "lang")((modname csclasswork4) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(define AreaTriangle
  (lambda (b h)
    (/ (* b h) 2))
  )
(AreaTriangle 5 5)

(define AreaSquare
  (lambda (a)
    (expt a 2))
  )
(AreaSquare 5)