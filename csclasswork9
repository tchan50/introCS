#reader(lib "htdp-advanced-reader.ss" "lang")((modname csclasswork9) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #t #t none #f () #f)))
(define isLeapYr?
  (lambda (y)
    (cond
      ((= (modulo y 400) 0) (print "true"))
      ((= (modulo y 100) 0) (print "false"))
      ((= (modulo y 4) 0) (print "true"))
      (else (print "false"))
      )
    )
  )
(isLeapYr? 1700)



    