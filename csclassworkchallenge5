#reader(lib "htdp-beginner-reader.ss" "lang")((modname csclassworkchallenge5) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(define isPythTriple?
  (lambda (a b c)
    (= (- (+ (expt a 2) (expt b 2) (expt c 2)) (expt (max a b c) 2)) (expt (max a b c) 2))
  )
)
(isPythTriple? 3 4 5)