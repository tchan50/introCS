#reader(lib "htdp-beginner-reader.ss" "lang")((modname csclasswork5) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(define isPythTriple?
  (lambda (s1 s2 s3)
    (= (+ (* s1 s1) (* s2 s2)) (* s3 s3)))
  )
 
(isPythTriple? 3 4 5)

