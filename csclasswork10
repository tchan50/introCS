#reader(lib "htdp-advanced-reader.ss" "lang")((modname csclasswork12) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #t #t none #f () #f)))
(define (CookieMonster n)
  (if (= n 0)
      (display "Do you have more cookies, I am still hungry!")
      (begin
        (display "Munch Munch Yum Yum Cookie")
        (display n)
        (newline)
        (CookieMonster (- n 1)))
  )
  )
(CookieMonster 5)
                 