;; about the language level of this file in a form that our tools can easily process.
#reader(lib "htdp-advanced-reader.ss" "lang")((modname cshomework10part2) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #t #t none #f () #f)))
(define (plusone y)
  (if (= y 12)
      (12)
      (+ y 1)
      )
  )
(define (TimesTable x)
  (if (= (plusone y) 12)
      (begin
        (display x)
        (display "*")
        (display y)
        (display "=")
        (display (* x 12)))
      (begin
        (display x)
        (display "*")
        (display y)
        (display "=")
        (display (* x y))
        (newline)
        (TimesTable x (+ y 1))
        )
      )
  )
(TimesTable 8 5)
                 